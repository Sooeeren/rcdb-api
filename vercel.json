{
  "version": 2,
  "builds": [
    {
      "src": "/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/coasters/random",
      "dest": "/api/coasters.js"
    },
    {
      "src": "/api/coasters/stat/(?<stat>[^/]+)",
      "dest": "/api/coasters.js"
    },
    {
      "src": "/api/coasters/(?<id>\\d+)",
      "dest": "/api/coasters.js"
    }
  ]
}